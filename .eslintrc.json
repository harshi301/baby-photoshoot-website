{
  "extends": ["next/core-web-vitals", "next", "prettier", "eslint:recommended"],
  "rules": {
    "react/no-unescaped-entities": "off",
    "quotes": ["warn", "double"],
    "array-callback-return": ["warn"],
    "consistent-return": ["warn"],
    "no-console": ["warn"],
    "no-duplicate-imports": ["warn"],
    "no-shadow": ["warn"],
    "quote-props": ["warn", "as-needed"]
  }
}
